{
    "name": "lulububu/html-minify",
    "description": "Minify HTML output",
    "type": "bolt-extension",
    "keywords": [
        "Minify HTML",
        "Site Optimization"
    ],
    "require": {
        "bolt/bolt": "^3.2"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Chris Hilsdon",
            "email": "chris@koolserve.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Lulububu\\HtmlMinify\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Lulububu\\HtmlMinify\\HtmlMinifyExtension",
        "bolt-icon": "icon.png"
    }
}
